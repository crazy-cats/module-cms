{
    "name": "crazycat/module-cms",
    "description": "CrazyCat Module - CMS",
    "type": "crazycat-module",
    "license": [
        "proprietary"
    ],
    "authors": [
        {
            "name": "Bruce Z",
            "email": "zengliwei@163.com",
            "homepage": "http://crazy-cat.co",
            "role": "Developer"
        }
    ],
    "require": {
        "crazycat/framework": "~1.0"
    },
    "require-dev": {
        "crazycat/framework": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "CrazyCat\\Cms\\": "code/"
        },
        "files": [
            "registration.php"
        ]
    },
    "scripts": {
        "post-package-install": [
            "php index.php system:cache:clear"
        ],
        "post-package-update": [
            "php index.php system:cache:clear"
        ],
        "post-package-uninstall": [
            "php index.php system:cache:clear"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}